{
  "name": "tlmpapp",
  "version": "1.0.0",
  "description": "A totally legit movie player",
  "author": "absurdoliver",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.skiwi.tlmpapp",
    "productName": "TLMP App",
    "win": {
      "target": "nsis",
      "icon": "assets/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "electron": "^26.0.0",
    "electron-builder": "^26.0.12"
  }
}
